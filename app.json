{
  "expo": {
    "name": "Meme Generator",
    "slug": "memegeneratorn",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "description": "A first go at React Native, calling an API and showing a randomly called meme, this now allows a user to create a meme using my imgflip account",
    "githubUrl": "https://github.com/Nathanavie/ReactNative"
  }
}
